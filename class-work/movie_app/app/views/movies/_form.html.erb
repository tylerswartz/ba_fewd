
<%= form_for @movie, url: {action: "create"} do |f|%>
  <%= f.label_tag(:title, "Title:") %>
  <%= f.text_field_tag :title, params[:title] %>
  <%= f.label_tag(:year_released, "Year Released:") %>
  <%= f.text_field_tag :year_released, params[:year_released] %>
  <%= f.label_tag(:description, "Description:") %>
  <%= f.text_field_tag :description, params[:description] %>
  <%= f.submit_tag 'Add Movie!' %>
<% end %> 